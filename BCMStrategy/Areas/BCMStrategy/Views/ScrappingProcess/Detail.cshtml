
@{
  ViewBag.ModuleName = BCMStrategy.Resources.Enums.ModuleName.SCRAPPINGPROCESS;
  Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
  .panel-heading {
    border-bottom: 1px solid #ccc;
    margin-bottom: 10px;
    display: block;
    background: #eee;
    padding: 8px 0 0 5px;
  }

  .panel-body {
    padding-left: 20px;
    margin-bottom: 15px;
  }

  tbody {
    /*height: 400px;  */
    overflow-y: auto; /* Trigger vertical scroll    */
    overflow-x: hidden; /* Hide the horizontal scroll */
  }

  table.table td, table.table th {
    padding-top: 0.1rem;
    padding-bottom: 0rem;
  }

  .content-right {
    float: right;
  }

  .clear-content {
    float: right;
  }

  @@media (max-width: 768px) {
    .content-right {
      float: left;
    }

    .clear-content {
      float: none;
    }
  }
</style>

<form id="scrappingProcess">
  <!--Main Layout-->
  <div class="container-fluid">
    <div class="mt-3">
      <div class="card">
       
        <div class="card-body">
          <div class="panel-group" id="accordion1">

          </div>
        </div>

      </div>
    </div>
  </div>

  <!--Main Layout-->
  @section scripts{
    @Scripts.Render("~/Scripts/JsAreas/ScrappingProcess.js")
    <script>
        // SideNav Button Initialization
        var webLinkHashId = '@ViewBag.WebSiteHashId';
        var webSiteType = '@ViewBag.WebSiteType';
        var processEventId = '@ViewBag.ProcessEventId';

        $(".button-collapse").sideNav();
        $(document).ready(function () {
            GetDetails(webLinkHashId, processEventId);
        })

    </script>
  }
</form>


